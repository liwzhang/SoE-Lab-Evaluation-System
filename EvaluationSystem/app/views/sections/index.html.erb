<p id="notice"><%= notice %></p>

<h1>Sections</h1>

<table>
  <thead>
    <tr>
      <th>Class num</th>
      <th>Professor Email</th>
      <th>Students Enrolled</th>
      <th>Evals Completed</th>
      <th>Subject</th>
      <th>Catalog</th>
      <th>Title</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sections.each do |section| %>
      <tr>
        <td><%= section.class_num %></td>
        <td><%= section.professor_email %></td>
        <td><%= section.enrolled %></td>
        <td><%= section.completed %></td>
        <td><%= section.subject %></td>
        <td><%= section.catalog %></td>
        <td><%= section.title %></td>
        <td><%= section.section %></td>
        <td><%= link_to 'Show', section %></td>
        <td><%= link_to 'Edit', edit_section_path(section) %></td>
        <td><%= link_to 'Destroy', section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section', new_section_path %>
